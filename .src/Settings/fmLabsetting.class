Private Function BarCodeFormats() As String[]

  Dim xfile As String
  Dim xlist As String[]

  xfile = modHelpVariable.$localesDirectory &/ "zint"
  If Exist(xfile) Then
    xlist = modString.GetSelStringFieldFromCSV(xfile, 1, ";")
  Endif
  Return xlist

End

