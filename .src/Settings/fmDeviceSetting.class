Public Sub btnussd_Click()

  Dim xx As String
  Dim msg As String

  Inc Application.Busy
  xx = modGSM.$ModemIndex
  If modGSM.$ModemEnabled = True Then
    msg = modGSM.USSDSession(xx, txtussd.Text)
    Wait 1
  Endif
  Dec Application.Busy

  Message.Info(msg, ("OK"))

End

