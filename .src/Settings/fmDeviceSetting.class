End

Public Sub btnpacsupload_Click()

  Dim FileList As String[]
  Dim xfile As String
  Dim xxx As String
  Dim xNew As String

  If txtpacshost.Text Then
    FileList = Dir(txtpacsupload.Text, Null, gb.File)
    Inc Application.Busy
    For Each xfile In FileList

      If Exist(txtpacsupload.Text &/ xfile) Then
        If chkencid.Value = True And If txtencid.Text Then
          xNew = modDevAll.ChangeEncIdDicom(Trim(txtencid.Text), txtpacsupload.Text &/ xfile)
          If Exist(xNew) Then
            xxx = modPACS.UploadDicomArchive(txtpacshost.Text, txtorthancport.Text, xNew, txtpacsuser.Text, txtpacspass.Text, cmbpacsssl.Text)
          Endif
        Else
          xxx = modPACS.UploadDicomArchive(txtpacshost.Text, txtorthancport.Text, txtpacsupload.Text &/ xfile, txtpacsuser.Text, txtpacspass.Text, cmbpacsssl.Text)
