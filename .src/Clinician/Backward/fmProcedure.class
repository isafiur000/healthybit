Private Sub LoadTabPicture(i As Integer)

  Dim p As Integer

  For p = 0 To TabPanel1.Count - 1
    If p = i Then
      TabPanel1[i].Picture = Picture["icons/coll4.png"]
    Else
      If TabPanel1[p].Picture Then
        TabPanel1[p].Picture = Picture[""]
      Endif
    Endif
  Next

End

