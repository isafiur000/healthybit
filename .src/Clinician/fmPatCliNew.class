  Else
    txttemp.Value = 0
  Endif
  If xresprate Then
    txtresprate.Value = xresprate
  Else
    txtresprate.Value = 0
  Endif
  If xoxysat Then
    txtoxysat.Value = xoxysat
  Else
    txtoxysat.Value = 0
  Endif

