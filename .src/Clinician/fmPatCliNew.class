Private Function GetReportIndex() As String

  Dim xIndex As String

  If txtclinIndex.Value And If txtclinmode.Text Then
    If txtclinmode.Text = "Consultation" Then
      xIndex = "Consultation|" & CStr(txtclinIndex.Value)
    Else If txtclinmode.Text = "OP Visit" Then
      xIndex = "OPVisit|" & CStr(txtclinIndex.Value)
    Endif
  Else
    xIndex = ""
  Endif

  Return xIndex

End

