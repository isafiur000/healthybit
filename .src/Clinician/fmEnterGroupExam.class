Public Sub _new(encid As String, sTable As String, sType As String)

  Dim ht As String

  $encid = encid
  $sTable = sTable
  $sType = sType

  $idList = New String[]
  AppFactor = modBasic.$AppScaleFactor
  ht = modSettings.ShowSettingFromFIle("MultipleTextBox/Height")
  If ht Then
    Slider1.Value = CInt(ht)
  Else
    Slider1.Value = 3
  Endif

End

Public Sub Form_Open()

  Dim xoption As String

  modGeneralMain.ArrangeFormCentre(Me, "False")
  If $sType = "Group" Then
    cmbcategory.List = modControlSub.GetDirectFillresult(modDatabase.$myConn.Exec("select distinct(fldgroupname) as col from tblgroupexam where fldcomp=&1 or fldcomp=&2", modBasic.$compID, "%"))                           ''
  Else If $sType = "Scales" Then
    cmbcategory.List = modControlSub.GetDirectFillresult(modDatabase.$myConn.Exec("select distinct(fldcategory) as col from tblexam where fldoption=&1", "Clinical Scale"))
  Else If $sType = "All" Then
    $compPhysiExamVar = GetPhysicianExamVariant()
    cmbcategory.List = modString.GetListFromVariant($compPhysiExamVar, "fldgroupname", True)
  Endif

  xoption = modSettings.ShowSettingFromFIle("EntrySetting/AutoDisplayOptions")
  If xoption = "Yes" Then
    chkautoption.Value = True
  Else If xoption = "No" Then
    chkautoption.Value = False
  Endif
  If cmbcategory.Count = 1 Then
    cmbcategory.Index = 0
    cmbcategory_Click()
  Endif

End

Public Sub Form_Close()

  modGeneralMain.DisableJavaScript()

End

Private Function GetPhysicianExamVariant() As Variant[]

  Dim sColl As Collection
  Dim aVar As Variant[]

  aVar = New Variant[]
  For Each sColl In modFixPatho.$ExaminationCompVar
    If sColl["fldcategory"] = "Physician Examinations" Then
      aVar.Add(sColl)
    Endif
  Next

  Return aVar

End

Public Sub cmbcategory_Click()

  Dim xht As Integer

  lblitem.Text = cmbcategory.Text
  btnBrOK.Enabled = True
  If lblitem.Text Then
    $idList.Clear()
    If Frame1.Children.Count Then
      Frame1.Children.Clear()
    Endif
    If $sType = "Group" Then
      $idList = modControlSub.GetDirectFillresult(modDatabase.$myConn.Exec("select fldexamid from tblgroupexam where fldgroupname=&1", lblitem.Text))
    Else If $sType = "Scales" Then
      $idList = modControlSub.GetDirectFillresult(modDatabase.$myConn.Exec("select fldexamid from tblexam where fldcategory=&1 and fldoption=&2", lblitem.Text, "Clinical Scale"))
    Else If $sType = "All" Then
      $idList = modString.GetListFromCompareVariant($compPhysiExamVar, "fldexamid", "fldgroupname", lblitem.Text, True)
      ' $idList = GetListExams()
    Endif
    If $idList.Count Then
      LoadFormData()
    Endif

    If $sExamVar Then
      xht = ($sExamVar.Count * 25 * Slider1.Value + 30) * AppFactor
      If Frame1.Height < xht Then
        Frame1.Height = xht
      Endif
    Endif
  Endif

End

Private Sub LoadFormData()

  Dim idx As String
  Dim res As Result
  Dim sColl As Collection

  Dim xType As String
  Dim xOption As String
  Dim xitem As String
  Dim xopList As Variant[]
  Dim xSysCons As String
  Dim xcritval As Float
  Dim aColl As Collection

  $sExamVar = New Variant[]

  For Each idx In $idList
    xopList = New Variant[]
    If $sType = "All" Then
      For Each sColl In $compPhysiExamVar
        If sColl["fldexamid"] = idx And If sColl["fldgroupname"] = cmbcategory.Text Then
          xitem = sColl["fldexamid"]
          xSysCons = sColl["fldsysconst"]
          xType = sColl["fldtype"]
          xOption = sColl["fldtanswertype"]
          If sColl["fldoptionlist"] Then
            xopList = JSON.Decode(sColl["fldoptionlist"])
          Else
            If sColl["fldtanswertype"] = "Dichotomous" Then
              xopList = ["No", "Yes"]
            Endif
          Endif
          Break
        Endif
      Next
      If xSysCons Then
        res = modDatabase.$myConn.Exec("select fldexamid,fldtype,fldoption,fldsysconst,fldcritical from tblexam where fldsysconst=&1", xSysCons)
        If res.Available Then
          res.MoveFirst
          xType = res["fldtype"]
          xSysCons = res["fldsysconst"]
          xOption = res["fldoption"]
          Select res["fldoption"]
            Case "Clinical Scale"
              xopList = modAllExam.GetCLinicalScaleOptions("Exam", res["fldexamid"])
            Case "Fixed Components", "Left/Right Components"
              xopList = modFixClinic.GetSubExamCollection(res["fldexamid"])
            Case Else
              xopList = modAllExam.SelectExamOptionList("Exam", res["fldexamid"])
          End Select

          If res["fldcritical"] Then
            xcritval = res["fldcritical"]
          Endif
        Endif
      Endif

    Else
      res = modDatabase.$myConn.Exec("select fldexamid,fldtype,fldoption,fldsysconst,fldcritical from tblexam where fldexamid=&1", idx)
      If res.Available Then
        xitem = res["fldexamid"]
        xSysCons = res["fldsysconst"]
        xType = res["fldtype"]
        xOption = res["fldoption"]
        Select res["fldoption"]
          Case "Clinical Scale"
            xopList = modAllExam.GetCLinicalScaleOptions("Exam", res["fldexamid"])
          Case "Fixed Components", "Left/Right Components"
            xopList = modFixClinic.GetSubExamCollection(res["fldexamid"])
          Case Else
            xopList = modAllExam.SelectExamOptionList("Exam", res["fldexamid"])
        End Select

        If res["fldcritical"] Then
          xcritval = res["fldcritical"]
        Endif
      Endif
    Endif

    aColl = New Collection
    aColl.Add(xitem, "ExamName")
    aColl.Add(xType, "DataType")
    aColl.Add(xOption, "OptionType")
    If xopList And If xopList.Count Then
      aColl.Add(xopList, "OptionList")
    Else
      xopList = New Variant[]
      aColl.Add(xopList, "OptionList")
    Endif
    aColl.Add(xSysCons, "SysConstant")
    If xcritval Then
      aColl.Add(xcritval, "CritValue")
    Else
      aColl.Add(0, "CritValue")
    Endif
    $sExamVar.Add(aColl)
  Next
  CreateControls()

End

Private Sub CreateControls()

  Dim i As Integer
  Dim j As Integer

  For i = 0 To $sExamVar.Count - 1
    If i < 49 Then
      ''Create controls
      aWebRowAll[i] = New HBox(Frame1)
      aWebFrame[i] = New HBox(aWebRowAll[i])
      aIndexLabel[i] = New Label(aWebFrame[i]) As "IndexLabel"
      aNameLabel[i] = New TextLabel(aWebFrame[i]) As "NameLabel"

      If $sExamVar[i]["DataType"] = "Quantitative" Then
        aWebPanel[i] = New HBox(aWebRowAll[i])
        aAbnormCheck[i] = New CheckBox(aWebPanel[i]) As "CheckBoxgroup"
        aQuantiBox[i] = New ValueBox(aWebPanel[i]) As "QuantiGroup"
        aQuantiMin[i] = New ValueBox(aWebPanel[i]) As "QuantiGroupMin"
        aQuantiMax[i] = New ValueBox(aWebPanel[i]) As "QuantiGroupMax"
        aQuantiUnit[i] = New Label(aWebPanel[i])
        aHelpButton[i] = New ToolButton(aWebPanel[i]) As "ButtonBoxgroup"
        aBlankBox[i] = New Label(aWebPanel[i])
        aWideRowAll[i] = New HBox(Frame1)
        aWebSpace[i] = New Separator(Frame1)

      Else
        Select $sExamVar[i]["OptionType"]
          Case "Sys Constant", "Multiple Selection", "Clinical Scale", "Date Time", "BS Date", "Single Selection", "Dichotomous", "Left Right Qualitative", "Left Right Quantitative", "Quantitative", "Qualitative", "Calculated", "ImageValue", "User Profile"                   ''
            aWebPanel[i] = New HBox(aWebRowAll[i])
            aAbnormCheck[i] = New CheckBox(aWebPanel[i]) As "CheckBoxgroup"
            Select $sExamVar[i]["OptionType"]
              Case "Sys Constant"
                aGenTextArea[i] = New TextPlain(aWebPanel[i]) As "GenAreaGroup"
              Case "Multiple Selection"
                aListBox[i] = New ListBox(aWebPanel[i]) As "ListBoxgroup"
              Case "Clinical Scale"
                aTextArea[i] = New TextArea(aWebPanel[i]) As "TextAreaGroup"
                aClinBox[i] = New ValueBox(aWebPanel[i]) As "ClinGroup"
              Case "Date Time", "BS Date"
                aDateBox[i] = New DateBox(aWebPanel[i]) As "DateGroup"
                aBSDate[i] = New ToolButton(aWebPanel[i]) As "BSButton"
              Case "Single Selection"
                aComboBox[i] = New ComboBox(aWebPanel[i]) As "ComboGroup"
              Case "Dichotomous"
                aDichoValue[i] = New DichotomousValue(aWebPanel[i]) As "DichoText"
              Case "Left Right Qualitative"
                aLeftRightText[i] = New LeftRightQuali(aWebPanel[i]) As "LeftRightText"
              Case "Left Right Quantitative"
                aLeftRightValue[i] = New LeftRightQuanti(aWebPanel[i]) As "LeftRightValue"
              Case "Quantitative"
                aValueBox[i] = New ValueBox(aWebPanel[i]) As "ValueGroup"
                aUnitText[i] = New TextBox(aWebPanel[i]) As "UnitGroup"
                aNullCheck[i] = New CheckBox(aWebPanel[i]) As "NullCheckBox"
              Case "Qualitative"
                aLineText[i] = New TextBox(aWebPanel[i]) As "TextLineGroup"
              Case "ImageValue"
                aButtonBox[i] = New ButtonBox(aWebPanel[i]) As "ImageBoxGroup"
              Case "User Profile"
                aLineText[i] = New TextBox(aWebPanel[i]) As "TextLineGroup"
              Case "Calculated"
                aCalcBox[i] = New ValueBox(aWebPanel[i]) As "CalcGroup"
                aCalcUnit[i] = New TextBox(aWebPanel[i]) As "CalcUnitGroup"
                aCalcNull[i] = New CheckBox(aWebPanel[i]) As "CalcCheckBox"
            End Select
            aHelpButton[i] = New ToolButton(aWebPanel[i]) As "ButtonBoxgroup"
            aBlankBox[i] = New Label(aWebPanel[i])
            aWideRowAll[i] = New HBox(Frame1)
            aWebSpace[i] = New Separator(Frame1)

          Case Else
            aWebPanel[i] = New HBox(aWebRowAll[i])
            aWideRowAll[i] = New HBox(Frame1)
            aAbnormCheck[i] = New CheckBox(aWideRowAll[i]) As "CheckBoxgroup"
            Select $sExamVar[i]["OptionType"]
              Case "Text Table"
                aGridView[i] = New GridView(aWideRowAll[i]) As "GridViewgroup"

              Case "Fixed Components", "Left/Right Components"
                aExpanFixed[i] = New VBox(aWideRowAll[i]) As "ExpanderGroup"
                For j = 0 To $sExamVar[i]["OptionList"].Count - 1
                  bHoriPanel[i, j] = New HBox(aExpanFixed[i])
                  cHoriPanel[i, j] = New VBox(aExpanFixed[i])
                  bExamLabel[i, j] = New Label(bHoriPanel[i, j])
                  bNameLabel[i, j] = New TextLabel(bHoriPanel[i, j])

                  Select $sExamVar[i]["OptionList"][j]["fldtanswertype"]
                    Case "Date Time", "BS Date"
                      bDateBox[i, j] = New DateBox(bHoriPanel[i, j])
                      bBSDate[i, j] = New ToolButton(bHoriPanel[i, j]) As "SubDateButton"
                    Case "Multiple Selection"
                      bListBox[i, j] = New ListBox(bHoriPanel[i, j])
                    Case "Clinical Scale"
                      bTextArea[i, j] = New TextArea(bHoriPanel[i, j])
                      bClinBox[i, j] = New ValueBox(bHoriPanel[i, j])
                    Case "Single Selection"
                      bComboBox[i, j] = New ComboBox(bHoriPanel[i, j]) As "SubComboGroup"
                    Case "Dichotomous"
                      bDichoValue[i, j] = New DichotomousValue(bHoriPanel[i, j])
                    Case "Left Right Qualitative"
                      bLeftRightText[i, j] = New LeftRightQuali(bHoriPanel[i, j])
                    Case "Left Right Quantitative"
                      bLeftRightValue[i, j] = New LeftRightQuanti(bHoriPanel[i, j])
                    Case "Quantitative"
                      bValueBox[i, j] = New ValueBox(bHoriPanel[i, j])
                      bUnitText[i, j] = New TextBox(bHoriPanel[i, j])
                      bNullCheck[i, j] = New CheckBox(bHoriPanel[i, j])
                    Case "Qualitative"
                      bLineText[i, j] = New TextBox(bHoriPanel[i, j])
                    Case "Left and Right"
                      bLeftRight[i, j] = New LeftRightTextArea(cHoriPanel[i, j])
                    Case "RichText Area"
                      bHTMLText[i, j] = New TextHTML(cHoriPanel[i, j])
                    Case Else
                      bGenTextArea[i, j] = New TextPlain(cHoriPanel[i, j])
                  End Select
                  bWebSpace[i, j] = New Separator(aExpanFixed[i])
                Next

              Case "Left and Right"
                aLeftRight[i] = New LeftRightTextArea(aWideRowAll[i]) As "LeftRightGroup"
              Case "RichText Area"
                aHTMLText[i] = New TextHTML(aWideRowAll[i]) As "TextHTMLgroup"
              Case Else
                aGenTextArea[i] = New TextPlain(aWideRowAll[i]) As "GenAreaGroup"
            End Select
            aHelpButton[i] = New ToolButton(aWideRowAll[i]) As "ButtonBoxgroup"
            aBlankBox[i] = New Label(aWideRowAll[i])
            aWebSpace[i] = New Separator(Frame1)

        End Select
      Endif

    Endif
  Next
  DIsplayTextForm()

End

Private Sub DIsplayTextForm()

  Dim i As Integer
  Dim xlimit As Float[]
  Dim asx As String[]
  Dim xcrit As Float

  Dim j As Integer
  Dim xopt As String
  Dim browHt As Integer
  Dim crowHt As Integer
  Dim sumHt As Integer

  For i = 0 To $sExamVar.Count - 1
    If i < 49 Then

      Select $sExamVar[i]["OptionType"]
        Case "Multiple Selection", "Clinical Scale"
          With aWebRowAll[i]
            .Height = 25 * Slider1.Value * AppFactor
            .Width = Frame1.Width
          End With
        Case Else
          With aWebRowAll[i]
            .Height = 25 * AppFactor
            .Width = Frame1.Width
          End With
      End Select

      If $sExamVar[i]["DataType"] = "Quantitative" Then
        With aWideRowAll[i]
          .Visible = False
        End With
      Else
        Select $sExamVar[i]["OptionType"]
          Case "Sys Constant", "Multiple Selection", "Clinical Scale", "Date Time", "BS Date", "Single Selection", "Dichotomous", "Left Right Qualitative", "Left Right Quantitative", "Quantitative", "Qualitative", "Calculated", "ImageValue", "User Profile"
            With aWideRowAll[i]
              .Visible = False
            End With
          Case "Fixed Components", "Left/Right Components"
          Case "Text Table", "RichText Area"
            With aWideRowAll[i]
              .Height = ((25 * Slider1.Value) + 50) * AppFactor
            End With
          Case Else
            With aWideRowAll[i]
              .Height = (25 * Slider1.Value) * AppFactor
            End With
        End Select
      Endif

      With aWebFrame[i]
        .Height = 25 * AppFactor
        .Expand = True
      End With

      ''create index label
      With aIndexLabel[i]
        .Width = 25 * AppFactor
        .Height = 25 * AppFactor
        .Text = i + 1
        .Tag = i
      End With

      ''create Name Label
      With aNameLabel[i]
        .Expand = True
        .Height = 25 * AppFactor
        .Text = "<p>" & $sExamVar[i]["ExamName"] & "</p>"
        .Font.Bold = True
        .Tag = i
        .Wrap = True
      End With

      If $sExamVar[i]["DataType"] = "Quantitative" Then
        With aWebPanel[i]
          .Height = 25 * AppFactor
          .Width = Frame1.Width / 2
        End With
      Else
        Select $sExamVar[i]["OptionType"]
          Case "Sys Constant", "Date Time", "BS Date", "Single Selection", "Dichotomous", "Left Right Qualitative", "Left Right Quantitative", "Quantitative", "Qualitative", "Calculated", "ImageValue", "User Profile"
            With aWebPanel[i]
              .Height = 25 * AppFactor
              .Width = Frame1.Width / 2
            End With
          Case "Multiple Selection", "Clinical Scale"
            With aWebPanel[i]
              .Height = 25 * Slider1.Value * AppFactor
              .Width = Frame1.Width / 2
            End With
          Case Else
            With aWebPanel[i]
              .Visible = False
            End With
        End Select
      Endif

      If $sExamVar[i]["DataType"] = "Quantitative" Then
        xlimit = modClinic.GetRangeQuantiExamSys($sExamVar[i]["SysConstant"], $encid)
        xcrit = $sExamVar[i]["CritValue"]
        ''create abnormal checkbox
        With aAbnormCheck[i]
          .Width = 75 * AppFactor
          .Height = 25 * AppFactor
          .Text = "Flag"
          .Enabled = False
          .Tag = i

          If modBasic.$ClinFlagAbnormExam = "Disable" Then
            .Visible = False
          Else
            .Visible = True
          Endif
        End With

        ''create observation entry
        With aQuantiBox[i]
          .Width = 75 * AppFactor
          .Height = 25 * AppFactor
          .Tag = i
        End With
        'min value
        With aQuantiMin[i]
          .Width = 50 * AppFactor
          .Height = 25 * AppFactor
          .Enabled = False
          .Value = xlimit[0]
          .Tag = i
        End With
        ''max value
        With aQuantiMax[i]
          .Width = 50 * AppFactor
          .Height = 25 * AppFactor
          .Enabled = False
          .Value = xlimit[1]
          .Tag = i
        End With
        With aQuantiUnit[i]
          .Width = 75 * AppFactor
          .Height = 25 * AppFactor
          .Border = Border.Plain
          .Text = ""
          .Tag = i
        End With

      Else
        ''create abnormal checkbox
        With aAbnormCheck[i]
          .Width = 75 * AppFactor
          .Height = 25 * AppFactor
          .Text = "Flag"
          .Tag = i

          If modBasic.$ClinFlagAbnormExam = "Disable" Then
            .Visible = False
          Else
            .Visible = True
          Endif
        End With

        ''create observation entry
        If $sExamVar[i]["OptionType"] = "Multiple Selection" Then
          With aListBox[i]
            .Expand = True
            .Height = 25 * Slider1.Value * AppFactor
            .Mode = Select.Multiple
            .ScrollBar = Scroll.Both
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Clinical Scale" Then
          With aTextArea[i]
            .Expand = True
            .Height = 25 * Slider1.Value * AppFactor
            .Wrap = True
            .Tag = i
          End With
          With aClinBox[i]
            .Width = 75 * AppFactor
            .Height = 25 * AppFactor
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Single Selection" Then
          With aComboBox[i]
            .Expand = True
            .Height = 25 * AppFactor
            .List = $sExamVar[i]["OptionList"]
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Dichotomous" Then
          With aDichoValue[i]
            .Expand = True
            .Height = 25 * AppFactor
            .List = $sExamVar[i]["OptionList"]
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Left Right Qualitative" Then
          With aLeftRightText[i]
            .Expand = True
            .Height = 25 * AppFactor
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Left Right Quantitative" Then
          With aLeftRightValue[i]
            .Expand = True
            .Height = 25 * AppFactor
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Date Time" Then
          With aDateBox[i]
            .Width = 200 * AppFactor
            .Height = 25 * AppFactor
            .Mode = DateChooser.DateTime
            .Tag = i
          End With
          With aBSDate[i]
            .Width = 30 * AppFactor
            .Height = 25 * AppFactor
            .Picture = Picture["icon:/small/calendar"]
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "BS Date" Then
          With aDateBox[i]
            .Width = 200 * AppFactor
            .Height = 25 * AppFactor
            .Mode = DateChooser.DateTime
            .Tag = i
          End With
          With aBSDate[i]
            .Width = 30 * AppFactor
            .Height = 25 * AppFactor
            .Picture = Picture["icon:/small/calendar"]
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Quantitative" Then
          With aValueBox[i]
            .Width = 100 * AppFactor
            .Height = 25 * AppFactor
            .Tag = i
          End With
          With aUnitText[i]
            .Width = 100 * AppFactor
            .Height = 25 * AppFactor
            .Tag = i
          End With
          With aNullCheck[i]
            .Width = 75 * AppFactor
            .Height = 25 * AppFactor
            .Tag = i
            .Value = False
            .Text = "Null"
          End With

        Else If $sExamVar[i]["OptionType"] = "ImageValue" Then
          With aButtonBox[i]
            .Expand = True
            .Height = 25 * AppFactor
            .Text = ""
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "User Profile" Then
          With aLineText[i]
            .Expand = True
            .Height = 25 * AppFactor
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Calculated" Then
          If $sExamVar[i]["SysConstant"] Then
            asx = modString.SplitValueText($sExamVar[i]["SysConstant"])
          Else
            asx = ["", ""]
          Endif
          With aCalcBox[i]
            .Width = 150 * AppFactor
            .Height = 25 * AppFactor
            .Tag = i
            If asx[0] Then
              .Value = CFloat(asx[0])
            Endif
          End With
          With aCalcUnit[i]
            .Width = 100 * AppFactor
            .Height = 25 * AppFactor
            .Tag = i
            .Text = asx[1]
          End With
          With aCalcNull[i]
            .Width = 75 * AppFactor
            .Height = 25 * AppFactor
            .Tag = i
            .Value = False
            .Text = "Null"
          End With

        Else If $sExamVar[i]["OptionType"] = "Qualitative" Then
          With aLineText[i]
            .Expand = True
            .Height = 25 * AppFactor
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Left and Right" Then
          With aLeftRight[i]
            .Expand = True
            .Height = (25 * Slider1.Value) * AppFactor
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "Fixed Components" Then
          ''----------- Fixed Components -----------------------
          sumHt = 0
          For j = 0 To $sExamVar[i]["OptionList"].Count - 1
            browHt = 0
            crowHt = 0

            xopt = $sExamVar[i]["OptionList"][j]["fldtanswertype"]
            Select xopt
              Case "Multiple Selection", "Clinical Scale"
                With bHoriPanel[i, j]
                  .Height = 25 * Slider1.Value * AppFactor
                  .Expand = True
                  browHt = browHt + (25 * Slider1.Value * AppFactor)
                End With
              Case Else
                With bHoriPanel[i, j]
                  .Height = 25 * AppFactor
                  .Expand = True
                  browHt = browHt + (25 * AppFactor)
                End With
            End Select

            Select xopt
              Case "Date Time", "BS Date", "Single Selection", "Dichotomous", "Left Right Qualitative", "Left Right Quantitative", "Quantitative", "Qualitative", "Clinical Scale", "Multiple Selection"
                With cHoriPanel[i, j]
                  .Visible = False
                End With
                ' Case "Text Table", "RichText Area"
                '   With cHoriPanel[i, j]
                '     .Height = (25 * Slider1.Value) * AppFactor
                '   End With
                ' Case Else
                '   With cHoriPanel[i, j]
                '     .Height = (25 * Slider1.Value) * AppFactor
                '   End With
            End Select

            With bExamLabel[i, j]
              .Width = 5 * AppFactor
              .Height = 25 * AppFactor
              .Text = $sExamVar[i]["ExamName"]
              .Visible = False
              .Tag = [i, j]
            End With
            With bNameLabel[i, j]
              .Width = (Frame1.Width - 150) / 2
              .Height = 25 * AppFactor
              .Text = "<p>" & $sExamVar[i]["OptionList"][j]["fldsubexam"] & "</p>"
              .Font.Bold = True
              .Tag = [i, j]
              .Wrap = True
            End With

            ''subcomponents
            If xopt = "Multiple Selection" Then
              With bListBox[i, j]
                .Expand = True
                .Height = 25 * Slider1.Value * AppFactor
                .Mode = Select.Multiple
                .ScrollBar = Scroll.Both
                .Tag = i
              End With
            Else If xopt = "Clinical Scale" Then
              With bTextArea[i, j]
                .Expand = True
                .Height = 25 * Slider1.Value * AppFactor
                .Wrap = True
                .Tag = [i, j]
              End With
              With bClinBox[i, j]
                .Width = 75 * AppFactor
                .Height = 25 * AppFactor
                .Tag = [i, j]
              End With
            Else If xopt = "Single Selection" Then
              With bComboBox[i, j]
                .Expand = True
                .Height = 28 * AppFactor
                .List = modAllExam.SelectSubExamOptionList("Exam", $sExamVar[i]["ExamName"], $sExamVar[i]["OptionList"][j]["fldsubexam"], xopt)
                .Tag = [i, j]
              End With
            Else If xopt = "Dichotomous" Then
              With bDichoValue[i, j]
                .Expand = True
                .Height = 25 * AppFactor
                .List = modAllExam.SelectSubExamOptionList("Exam", $sExamVar[i]["ExamName"], $sExamVar[i]["OptionList"][j]["fldsubexam"], xopt)
                .Tag = [i, j]
              End With
            Else If xopt = "Left Right Qualitative" Then
              With bLeftRightText[i, j]
                .Expand = True
                .Height = 25 * AppFactor
                .Tag = [i, j]
              End With
            Else If xopt = "Left Right Quantitative" Then
              With bLeftRightValue[i, j]
                .Expand = True
                .Height = 25 * AppFactor
                .Tag = [i, j]
              End With
            Else If xopt = "Date Time" Then
              With bDateBox[i, j]
                .Width = 200 * AppFactor
                .Height = 25 * AppFactor
                .Mode = DateChooser.DateTime
                .Tag = [i, j]
              End With
              With bBSDate[i, j]
                .Width = 30 * AppFactor
                .Height = 25 * AppFactor
                .Picture = Picture["icon:/small/calendar"]
                .Tag = [i, j]
              End With
            Else If xopt = "BS Date" Then
              With bDateBox[i, j]
                .Width = 200 * AppFactor
                .Height = 25 * AppFactor
                .Mode = DateChooser.DateTime
                .Tag = [i, j]
              End With
              With bBSDate[i, j]
                .Width = 30 * AppFactor
                .Height = 25 * AppFactor
                .Picture = Picture["icon:/small/calendar"]
                .Tag = [i, j]
              End With
            Else If xopt = "Quantitative" Then
              With bValueBox[i, j]
                .Width = 100 * AppFactor
                .Height = 25 * AppFactor
                .Tag = [i, j]
              End With
              With bUnitText[i, j]
                .Width = 100 * AppFactor
                .Height = 25 * AppFactor
                .Tag = [i, j]
              End With
            Else If xopt = "Qualitative" Then
              With bLineText[i, j]
                .Expand = True
                .Height = 25 * AppFactor
                .Tag = [i, j]
              End With
            Else If xopt = "Left and Right" Then
              With bLeftRight[i, j]
                .Expand = True
                .Height = (25 * Slider1.Value) * AppFactor
                .Tag = [i, j]
                crowHt = crowHt + ((25 * Slider1.Value) * AppFactor)
              End With
            Else If xopt = "RichText Area" Then
              With bHTMLText[i, j]
                .Expand = True
                .Height = ((25 * Slider1.Value) + 50) * AppFactor
                .DictionaryPath = modBasic.$dictPathList
                .Tag = [i, j]
                crowHt = crowHt + (((25 * Slider1.Value) + 50) * AppFactor)
              End With
            Else
              With bGenTextArea[i, j]
                .Expand = True
                .Height = (25 * Slider1.Value) * AppFactor
                .DictionaryPath = modBasic.$dictPathList
                .Tag = [i, j]
                crowHt = crowHt + ((25 * Slider1.Value) * AppFactor)
              End With
            Endif

            With cHoriPanel[i, j]
              .Height = crowHt
            End With

            With bWebSpace[i, j]
              .Height = 15 * AppFactor
            End With

            sumHt = sumHt + crowHt + (40 * AppFactor)
          Next

          With aExpanFixed[i]
            .Expand = True
            .Height = sumHt + (25 * AppFactor)
            .Tag = i
          End With
          With aWideRowAll[i]
            .Height = sumHt + (50 * AppFactor)
          End With
          ''-------- end of fixed components ----------------

        Else If $sExamVar[i]["OptionType"] = "Left/Right Components" Then

        Else If $sExamVar[i]["OptionType"] = "Text Table" Then
          DisplayAgegrid($sExamVar[i]["OptionList"], "", aGridView[i])
          With aGridView[i]
            .Expand = True
            .Height = ((25 * Slider1.Value) + 50) * AppFactor
            .ScrollBar = Scroll.Both
            .AutoResize = True
            .Tag = i
          End With

        Else If $sExamVar[i]["OptionType"] = "RichText Area" Then
          With aHTMLText[i]
            .Expand = True
            .Height = ((25 * Slider1.Value) + 50) * AppFactor
            .DictionaryPath = modBasic.$dictPathList
            .Tag = i
          End With

        Else
          With aGenTextArea[i]
            .Expand = True
            .Height = (25 * Slider1.Value) * AppFactor
            .DictionaryPath = modBasic.$dictPathList
            .Tag = i
          End With

        Endif
      Endif

      ''create help button
      With aHelpButton[i]
        .Width = 25 * AppFactor
        .Height = 25 * AppFactor
        .Text = ""
        .Picture = Picture["icon:/small/info"]
        .Tag = i
        .Tooltip = "[Ctrl+O] to display Options"
      End With
      If $sExamVar[i]["OptionType"] = "Fixed Components" Or If $sExamVar[i]["OptionType"] = "Left/Right Components" Then
        aHelpButton[i].Enabled = False
      Endif

      With aBlankBox[i]
        .Height = 25 * AppFactor
        .Width = 15 * AppFactor
      End With

      With aWebSpace[i]
        .Height = 15 * AppFactor
      End With

    Endif
  Next

