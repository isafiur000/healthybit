    Else If TabStrip1.Index = 4 Then
      dtnote.Value = Now()
      lbldtnote.Text = modPatient.GetDayDiffAdmission(Trim(txtencid.Text), Now())
      ShowNoteTypeTime()
    Else If TabStrip1.Index = 5 Then
      rbvalue.Value = True
      ShowMedicines("Current")
    Else If TabStrip1.Index = 6 Then
      rbvalueR.Value = True
