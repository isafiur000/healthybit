            modPharmSub.InsertNurDosing(grdfluid[grdfluid.Row, 0].Text, Trim(txtencid.Text), yval, "mL/Hour", Now())
            FillFluidGridView(grdfluid[grdfluid.Row, 0].Text, grdfluid[grdfluid.Row, 2].Text)
            ShowMedFluids("Current")
