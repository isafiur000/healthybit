      yval = InputValue(grdfluid[grdfluid.Row, 2].Text, ("Select Number of Days"), grdfluid[grdfluid.Row, 5].Text)
      If yval Then
        res = modDatabase.$myConn.Edit("tblpatdosing", "fldid=&1", grdfluid[grdfluid.Row, 0].Text)
