Private Sub GetMarkAbnorm(txtnoteidx As ValueBox, sRange As Float[])

  Dim xmax As Float
  Dim xmin As Float
  Dim abn As Boolean

  xmin = sRange[0]
  xmax = sRange[1]

  abn = False
  If txtnoteidx.Value Then
    If xmin Then
      If xmin > txtnoteidx.Value Then
        abn = True
      Endif
    Endif
    If xmax Then
      If xmax < txtnoteidx.Value Then
        abn = True
      Endif
    Endif
  Endif

  If abn = True Then
    txtnoteidx.Font.Bold = True
    txtnoteidx.Foreground = Color.Red
  Else
    txtnoteidx.Font.Bold = False
    txtnoteidx.Foreground = Color.Default
  Endif

End

