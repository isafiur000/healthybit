Private Sub ShowNoteTypeTime()

  Dim sql As String

  sql = "select fldid,fldtime,flditem,fldreportquali,flduserid,fldtime,flddetail from tblexamgeneral where fldencounterval=&1 and fldinput=&2 and flditem=&3 ORDER BY fldtime DESC"
  $rData = modDatabase.$myConn.Exec(sql, Trim(txtencid.Text), "Notes", "Doctor Order")
  $aMyFields = New String[]
  modGridView.ReadSmallData(grdnote, $rData, $aMyFields)

  With grdnote
    .Columns[0].Width = 1
    .Columns[1].Width = 150 * modBasic.$AppWidthRatio
    .Columns[2].Width = 1
    .Columns[3].Width = 200 * modBasic.$AppWidthRatio
    .Columns[4].Width = 75 * modBasic.$AppWidthRatio
    .Columns[5].Width = 1
    .Columns[6].Width = 1

    .Columns[1].Text = "DateTime"
    .Columns[3].Text = "Response"
    .Columns[4].Text = "Doctor"
  End With

End

Public Sub grdnote_Data(Row As Integer, Column As Integer)

