Private Function GetProceedBool(sComm As String, sVal As String) As Boolean

  Dim xx As Boolean

  If sComm = sVal Then
    xx = True
  Else
    If Message.Question("Visit Date doesn't match protocol. Do you want to add visit ?", ("No"), ("Yes")) = 2 Then
      xx = True
    Else
      xx = False
    Endif
  Endif

  Return xx

End

