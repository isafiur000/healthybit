Private Function GetBillingSSFItemAbbvName(itemName As String) As String

  Dim asx As String[]
  Dim xxx As String

  asx = Split(itemName, "|")
  If asx.Count Then
    Select asx[0]
      Case "Diagnostic Tests", "Radio Diagnostics", "General Services", "Procedures", "Equipment"
        xxx = modNonMedical.GetBillItemHIAbbv(asx[1], asx[0])
      Case Else
        xxx = "ADJ02"
    End Select
  Else
    xxx = ""
  Endif
  Return xxx

End

