          res["fldptbirday"] = $DOBirth
        Endif
        res["fldptcode"] = txtpatcode.Text
        res["fldadmitfile"] = ""
        res["fldupuser"] = modBasic.$lbluser
        res["flduptime"] = Now()
        res["xyz"] = False
        res.Update()
        modPatientSub.RemovePatientCache(Trim(txtpatno.Text))
        FillRegistrationGrid()
        Balloon.Info(("Information updated"), btnChange)
