Private Sub FillDataViewGrid()

  Dim i As Integer
  Dim Column As Integer

  $aMyFieldsv = New String[]
  modGridView.ReadSmallData(GridView2, $rDatav, $aMyFieldsv)
  With GridView2
    .Columns[0].Width = 125 * modBasic.$AppWidthRatio
    .Columns[1].Width = 175 * modBasic.$AppWidthRatio
    .Columns[2].Width = 75 * modBasic.$AppWidthRatio
    .Columns[3].Width = 75 * modBasic.$AppWidthRatio
    .Columns[0].Text = "EncID"
    .Columns[1].Text = "Name"
    .Columns[2].Text = "Age"
    .Columns[3].Text = "Gender"

    For Column = 0 To $varList.Count - 1
      .Columns[Column + 4].Width = 150 * modBasic.$AppWidthRatio
      .Columns[Column + 4].Text = $varList[Column]
    Next

    If $newColumn.Count Then
      For i = 0 To $newColumn.Count - 1
        .Columns[$varList.Count + 4 + i].Text = $newColumn[i]
        .Columns[$varList.Count + 4 + i].Width = 150 * modBasic.$AppWidthRatio
      Next
    Endif
  End With

End

Public Sub GridView2_Data(Row As Integer, Column As Integer)

  $rDatav.MoveTo(Row)
  modGeneralMain.GridViewDecoration(GridView2, Row)
  If Column > ($varList.Count + 4) - 1 Then
    GridView2.Data.RichText = modString.TextToHTML(GetDataViewValue(Column, $rDatav[$aMyFieldsv[Column]]))
  Else
    GridView2.Data.Text = GetDataViewValue(Column, $rDatav[$aMyFieldsv[Column]])
  Endif

End

Private Function GetDataViewValue(Column As Integer, xVariable As Variant) As Variant

  Dim xxx As Variant
  Dim i As Integer
  Dim aColl As Collection

  If $dataVar.Exist(xVariable) = False Then
    $dataVar.Add(GetLabCollection(xVariable), xVariable)
  Endif
  If Column = 0 Then
    xxx = xVariable
  Else If Column = 1 Then
    xxx = modPatient.GetPatientNameByEnc(xVariable, modDatabase.$syConn)
  Else If Column = 2 Then
    xxx = modMedReports.GetPatientPastAgeString("tblencounter", xVariable)
  Else If Column = 3 Then
    xxx = modPatient.GetPatientSex(xVariable, modDatabase.$syConn)
  Else
    If Column < $varList.Count + 4 Then
      aColl = $dataVar[xVariable]
      If $varList[Column - 4] And If aColl.Exist($varList[Column - 4]) Then
        xxx = aColl[$varList[Column - 4]]
      Else
        xxx = ""
      Endif
    Endif
  Endif

  If $newColumn.Count Then
    For i = 0 To $newColumn.Count - 1
      If Column = $varList.Count + 4 + i Then
        xxx = modCustPatient.NewColValue(Me.Tag, $newColumn[i], xVariable)
      Endif
    Next
  Endif
  Return xxx

End

Private Function GetLabCollection(encid As String) As Collection

  Dim res As Result
  Dim aColl As Collection

  aColl = New Collection
  res = ExecuteQuery(["fldtestid", "fldreportquali"], "fldencounterval|" & encid & "|ORDER BY fldtime_sample")
  If res.Available Then
    For Each res
      If aColl.Exist(res["fldtestid"]) = False Then
        aColl.Add(res["fldreportquali"], res["fldtestid"])
      Endif
    Next
  Endif

  Return aColl

End

Public Sub mnumultivariate_Click()

  Dim CForm As CMultivariate
  Dim sArry As String[]
  Dim Row As Integer
  Dim Column As Integer
  Dim sPath As String
  Dim xHeader As String[]
  Dim hPython As FmPythonTerminal

  If MMain.$IsGUIApp = True Then
    $ProgressBar1 = modAppSupport.FindWorkProgressBar(modHelpVariable.$LogInCategory)
    $ProgressBar1.Visible = True
    $ProgressBar1.Value = 0
  Endif

  If GridView2.Rows.Count Then
    xHeader = New String[]
    For Column = 0 To GridView2.Columns.Count - 1
      xHeader.Add(GridView2.Columns[Column].Text)
    Next

    Inc Application.Busy
    CForm = New CMultivariate
    For Row = 0 To GridView2.Rows.Count - 1
      sArry = New String[]
      For Column = 0 To xHeader.Count - 1
        If Column < $varList.Count + 4 Then
          sArry.Add(GridView2[Row, Column].Text)
        Else
          sArry.Add(modString.ConvertRichTextToPlain(GridView2[Row, Column].RichText))
        Endif
      Next
      CForm.Add(sArry)
      If MMain.$IsGUIApp = True Then
        $ProgressBar1.Value = (Row + 1) / GridView2.Rows.Count
        Wait
      Endif
    Next
    sPath = CForm.GetCSVFile()
    Dec Application.Busy
    If MMain.$IsGUIApp = True Then
      $ProgressBar1.Visible = False
    Endif

    If Exist(sPath) Then
      hPython = New FmPythonTerminal("Multivariate", xHeader, sPath)
      modWorkSpace.Add(hPython)
    Endif
  Endif

End
