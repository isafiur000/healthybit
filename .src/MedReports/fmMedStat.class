  Else

    If cmbcategory.Text = "Final Diagnosis Code" Then
      If MMain.$WebReport = "Multiple" Then
        sql = "select distinct(fldcodenew) as col from tblpatfindings where fldtype=&1 and fldsave=&2" & $RepoStr
