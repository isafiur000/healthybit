Private Function GetLabCollection(encid As String) As Collection

  Dim res As Result
  Dim aColl As Collection
  Dim aList As String[]
  Dim i As Integer

  aList = New String[]
  aList = $LabList.Copy()
  If aList And If aList.Count Then
    For i = 0 To aList.Count - 1
      aList[i] = "'" & aList[i] & "'"
    Next

    aColl = New Collection
    res = modDatabase.$syConn.Exec("select fldtestid,fldreportquali from tblpatlabtest where fldencounterval=&1 and (fldstatus=&2 or fldstatus=&3) and fldtestid in(" & aList.Join(",") & ") ORDER BY fldtime_sample ASC", encid, "Reported", "Verified")
    If res.Available Then
      For Each res
        If aColl.Exist(res["fldtestid"]) = False Then
          aColl.Add(res["fldreportquali"], res["fldtestid"])
        Endif
      Next
    Endif
  Endif

  Return aColl

End

