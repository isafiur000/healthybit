  If cmbcategory.Text Then
    txtdictext.RichText = GetRichTextArea(cmbcategory.Text, txtdictext.RichText)
  Endif

End

Public Sub btndictadd_Click()

  Dim ConfFile As Settings
  Dim xpath As String

  If txtdicttile.Text Then
    xpath = txtdictpath.Text
    If Exist(xpath) Then
      If cmbcategory.Text Then
        ConfFile = New Settings(modSettings.ShowSettingFromFIle("TextTemplate/Path"))
        ConfFile[cmbcategory.Text & "/" & txtdicttile.Text] = Trim(txtdictext.RichText)
        ConfFile.Save()
        lstdict.List = SHowAdvanceDictItems(txtdictpath.Text, cmbcategory.Text)
        ShowTemplateCategories(txtdictpath.Text)
        Balloon.Info(("Information saved"), btndictadd)
        Balloon.Delay = modBasic.$BalloonDelay
      Endif
    Endif
  Endif

End

