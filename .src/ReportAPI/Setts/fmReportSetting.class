Private Sub ShowPDFVariable()

  Dim xx As String

  xx = "[page]:number of pages currently being printed" & gb.NewLine
  xx = xx & "[frompage]:number of first page to be printed" & gb.NewLine
  xx = xx & "[topage]:number of last page to be printed" & gb.NewLine
  xx = xx & "[date]:current date in system local format" & gb.NewLine
  xx = xx & "[time]:current time in system local format"
  Message.Info(xx, ("OK"))

End

