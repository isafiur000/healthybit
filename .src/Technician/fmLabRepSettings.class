Private Sub EnableButton(sval As Boolean)

  If sval = True Then
    btnrecord.Enabled = True
    If modBasic.$LabSMSReportLock = "Verified" Then
      If rbverified.Value = True
        btnftp.Enabled = True
      Else
        btnftp.Enabled = False
      Endif
    Else
      btnftp.Enabled = True
    Endif
    btnreport.Enabled = True
    btnheadless.Enabled = True
  Else If sval = False Then
    btnrecord.Enabled = False
    btnftp.Enabled = False
    btnreport.Enabled = False
    btnheadless.Enabled = False
  Endif

End

Private Sub EnableSelected()

  Dim sval As Integer
  Dim i As Integer

  sval = 0
  For i = 0 To GridView1.Rows.Count - 1
    If GridView1[i, 1].Picture = Picture["icons/checked.png"] Then
      sval = sval + 1
    Endif
  Next
  If sval Then
    EnableButton(True)
  Else
    EnableButton(False)
  Endif

End

Private Sub DefaultTestReportFormat()

  Dim def As String

  def = modSettings.ShowSettingFromFIle("TestReportFormat/Format")
  If def Then
    If def = "A" Then
      rbform1.Value = True
    Else If def = "B" Then
      rbform2.Value = True
    Else If def = "C" Then
      rbform3.Value = True
    Else If def = "D" Then
      rbform4.Value = True
    Endif
  Else
    rbform2.Value = True
  Endif

End

