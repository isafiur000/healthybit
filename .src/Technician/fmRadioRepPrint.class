Public Sub GridView1_Data(Row As Integer, Column As Integer)

  $rData.MoveTo(Row)
  modGeneralMain.GridViewDecoration(GridView1, Row)
  If Column = 1 Then
    GridView1.Data.Picture = Picture["icons/unchecked.png"]
    GridView1.Data.Text = ""
  Else If Column = 2 Then
    GridView1.Data.Picture = Picture["icons/unchecked.png"]
    GridView1.Data.Text = ""
  Else If Column = 6 Then
    GridView1.Data.Text = modReportVar.GetDateTimeReport($rData[$aMyFields[Column]], gb.GeneralDate)
  Else If Column = 7 Then
    GridView1.Data.Picture = Picture[modMisc.GetGridIcon($rData[$aMyFields[Column]])]
    GridView1.Data.Text = ""
  Else If Column = 8 Then
    GridView1.Data.RichText = modRadioTest.GetRadioTestValueString($rData[$aMyFields[Column]], True)
    If modBasic.$RichtextResizeRow = "Yes" Then
      GridView1.Rows[Row].Height = Max(GridView1.Rows[Row].Height, GridView1.Data.Font.RichTextHeight(GridView1.Data.RichText, GridView1.Columns[Column].Width - 5 * modBasic.$AppWidthRatio) + (GridView1.Rows.Height - GridView1.Font.Height))
      GridView1.Data.WordWrap = True
    Endif
  Else If Column = 16 Then
    GridView1.Data.Picture = Picture[modMisc.SetGridCheckIcon($rData[$aMyFields[Column]])]
    GridView1.Data.Text = ""
  Else If Column = 18 Then
    GridView1.Data.Text = modFixRadio.GetRadioTestCategory($rData[$aMyFields[Column]])
  Else
    GridView1.Data.Text = $rData[$aMyFields[Column]]
  Endif

End

Private Sub FillTestAutoFooter(TestName As String)

  Dim xfoot As String

  If modBasic.$LabAutoFooter = "Database" Then
    xfoot = modFixRadio.GetRadioToolTip(TestName)
    If xfoot Then
      If Not txtcomment.Text Then
        txtcomment.RichText = xfoot
      Else
        If txtcomment.RichMode = True Then
          txtcomment.RichText = txtcomment.RichText & "<br>" & xfoot
        Else
          txtcomment.RichText = txtcomment.RichText & gb.NewLine & xfoot
        Endif
      Endif
    Endif
  Endif

End

Public Sub SelectRowSingle()

