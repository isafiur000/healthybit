        .Text = i + 1
        .Visible = False
        .Tag = i
      End With

      ''create Name Label
      With aNameLabel[i]
        .Expand = True
        .Height = 25 * AppFactor
        If $xData[i][3] = "Label Only" Then
          .Text = UCase($xData[i][0])
        Else
          .Text = "<p>" & $xData[i][0] & "</p>"
