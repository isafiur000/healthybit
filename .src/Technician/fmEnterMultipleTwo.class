      If $xData[i][3] = "Label Only" Then
        With aWebPanel[i]
          .Visible = False
        End With
      Else
        With aWebPanel[i]
          .AutoResize = True
          .Margin = True
          .Width = Frame1.Width
        End With
      Endif

      ''create abnormal checkbox
      If $xData[i][3] = "Drug Sensitivity" Or If $xData[i][3] = "WHO Sensitivity" Then
