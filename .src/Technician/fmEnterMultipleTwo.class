Private Function GetListForControl(sItem As String, sSelection As String) As String[]

  Dim xList As String[]

  xList = modAllExam.SelectSubExamOptionList($frm, lblitem.Text, sItem, sSelection)

  Return xList

End

Public Sub CheckBoxgroup_Click()

  Dim j As Integer

  j = Last.Tag
  If aAbnormCheck[j].Value = True Then
    aAbnormCheck[j].Foreground = Color.Red
  Else
    aAbnormCheck[j].Foreground = Color.Default
  Endif

End

Private Sub GetFocusControl(i As Integer)

  If aHTMLTextR[i] Then
    ScrollView1.EnsureVisible(aHTMLTextR[i].X, aHTMLTextR[i].Y, aHTMLTextR[i].Width, aHTMLTextR[i].Height)
    aHTMLTextR[i].SetFocus
  Else If aLineTextR[i] Then
    ScrollView1.EnsureVisible(aLineTextR[i].X, aLineTextR[i].Y, aLineTextR[i].Width, aLineTextR[i].Height)
    aLineTextR[i].SetFocus
  Else If aComboBoxR[i] Then
    ScrollView1.EnsureVisible(aComboBoxR[i].X, aComboBoxR[i].Y, aComboBoxR[i].Width, aComboBoxR[i].Height)
