Private Function GetRefRange(sRange As String) As Float[]

  Dim asx As String[]
  Dim bsx1 As String[]
  Dim bsx2 As String[]
  Dim afinal As Float[]

  afinal = New Float[]
  If sRange Then
    asx = Split(sRange, "-")
    bsx1 = Split(Trim(asx[0]), " ")
    Try afinal.Add(CFloat(Trim(bsx1[0])))
    If asx.Count > 1 Then
      bsx2 = Split(Trim(asx[1]), " ")
      Try afinal.Add(CFloat(Trim(bsx2[0])))
    Endif
  Endif
  Return afinal

End

