Private Sub PrintBarCodeSampling(xSampleID As String, xSpecimen As String)

  Dim i As Integer
  Dim xval As String[]
  Dim xsect As String

  Dim xx As String

  If xSampleID And If $encid Then
    If GridView1.Rows.Count Then

      xval = New String[]
      For i = 0 To GridView1.Rows.Count - 1
        If GridView1[i, 1].Picture = Picture["icons/checked.png"] Then

          If modBasic.$LabBarPrint = "Section" Then
            xsect = modFixLab.GetLabTestCategory(GridView1[i, 3].Text)
          Else If modBasic.$LabBarPrint = "TestName" Then
            xsect = GridView1[i, 3].Text
          Else If modBasic.$LabBarPrint = "BillItem" Or If modBasic.$LabBarPrint = "Section/BillItem" Then
            xsect = GridView1[i, 2].Text
          Else
            xsect = ""
          Endif

          If xsect Then
            If xval.Count = 0 Then
              xval.Add(xsect)
            Else
              If xval.Exist(xsect) = False Then
                xval.Add(xsect)
              Endif
            Endif
          Endif

        Endif
      Next

      If xval.Count Then
        If modBasic.$LabBarPrint = "Section/BillItem" Then
          modLabSub.PrintBarLabDeptSampling($encid, xSampleID, xval, txtencid.Text)
        Else
          For Each xx In xval
            modLabSub.PrintBarCodeSampling($encid, xSampleID, xSpecimen, xx, txtencid.Text)
          Next
        Endif
      Else
        modLabSub.PrintBarCodeSampling($encid, xSampleID, xSpecimen, "", txtencid.Text)
      Endif

    Endif
  Endif

End

