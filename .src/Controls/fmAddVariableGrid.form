# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,50,63)
  Text = ("Variables")
  Resizable = False
  { txtvariable TextBox
    MoveScaled(1,1,48,3.5333)
  }
  { btnOK Button
    MoveScaled(1,5,13,3.5333)
    Text = Shortcut(("Add"), "A")
    Picture = Picture["icon:/small/add"]
    Default = True
  }
  { btnCancel Button
    MoveScaled(36,5,13,3.5333)
    Text = Shortcut(("Delete"), "D")
    Picture = Picture["icon:/small/cancel"]
  }
  { GridView1 GridView
    MoveScaled(1,9,48,49)
    Mode = Select.Single
  }
  { txtcsv ButtonBox
    MoveScaled(1,59,36,3)
    ToolTip = ("Select a text file with ONE column of items to import. Field name of the table not required")
    Picture = Picture["icon:/small/file"]
  }
  { btnupload Button
    MoveScaled(38,59,11,3)
    Text = ("Import")
    Picture = Picture["icon:/small/copy"]
  }
}
