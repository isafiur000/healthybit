Private Sub FillUserList()

  Dim pic1 As Picture
  Dim xuser As String
  Dim xuserList As String[]

  xuserList = GetSelUserList()
  pic1 = Picture["icon:/small/identity"]
  lstitem.Clear()
  For Each xuser In xuserList
    If xuser Then
      lstitem.Add(xuser, modGeneral.GetUserFullName(xuser), pic1)
    Endif
  Next

End

